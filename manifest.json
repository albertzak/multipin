{
  "name": "Multi Pin",
  "short_name": "MultiPin",
  "version": "0.0.3",
  "manifest_version": 2,
  "description": "Select multiple pins to pin on Pinterest",
  "homepage_url": "http://albertzak.com",
  "icons": {
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "permissions": [
    "https://*.pinterest.com/*/*",
    "http://*.pinterest.com/*/*"
  ],
  "web_accessible_resources": [
    "src/multipin.js"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*.pinterest.com/*/*",
        "http://*.pinterest.com/*/*"
      ],
      "js": [
        "src/inject/inject.js"
      ],
      "css": [
        "src/inject/inject.css"
      ]
    }
  ]
}
